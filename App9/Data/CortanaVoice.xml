<?xml version="1.0" encoding="utf-8"?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.1">
  <CommandSet xml:lang="en-us" Name="App9CommandSet_en-us">
    <CommandPrefix> Hello app, </CommandPrefix>
    <Example> In App9 </Example>

    <Command Name="blankCanvas">
      <Example> Give me a blank canvas with red background </Example>
      <ListenFor> [with] {color} background </ListenFor>
      <ListenFor> Give me a blank canvas with [a] {color} background </ListenFor>
      <ListenFor> Give me a blank {color} canvas </ListenFor>
      <Feedback> Giving you a blank {color} canvas </Feedback>
      <Navigate/>
    </Command>

    <PhraseList Label="color">
      <Item>Red</Item>
      <Item>Blue</Item>
      <Item>Yellow</Item>
      <Item>Green</Item>
    </PhraseList>

  </CommandSet>

  <!-- Other CommandSets for other languages -->

</VoiceCommands>